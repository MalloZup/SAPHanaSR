cDir="/home/fabian/tmp/bug-Ilya/hawk-2015-11-20T12:10:53+00:00-2015-11-20T13:10:53+00:00/dhcp24/pengine"
sid="sle"
mkdir -p $cDir/tmp
ls $cDir
for cibBZ2 in $cDir/*.bz2; do 
   cib=${cibBZ2%.bz2}; 
   echo "cib: $cib"; 
   cp $cibBZ2 $cDir/tmp/ACIB.bz2 ; 
   bzip2 -d $cDir/tmp/ACIB.bz2; 
#   ./SAPHanaSR-monitor  --cib=$cDir/tmp/ACIB --sid=cp1; 
   ./SAPHanaSR-showAttr --cib=$cDir/tmp/ACIB --sid=$sid; 
   rm $cDir/tmp/ACIB ;    
done 
