

cDir=~/tmp/bz-951636/pcmk-Tue-20-Oct-2015/mn600d5a002/pengine; 
for cibBZ2 in $cDir/*.bz2; do 
   cib=${cibBZ2%.bz2}; 
   echo "cib: $cib"; 
   cp $cibBZ2 $cDir/tmp/ACIB.bz2 ; 
   bzip2 -d $cDir/tmp/ACIB.bz2; 
   ./SAPHanaSR-monitor  --cib=$cDir/tmp/ACIB --sid=cp1; 
   ./SAPHanaSR-showAttr --cib=$cDir/tmp/ACIB --sid=cp1; 
   rm $cDir/tmp/ACIB ;    
done  > ~/tmp/bz-951636/SAPHanaSR-REPLAY.txt
